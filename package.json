{
  "name": "kable-internals",
  "version": "0.0.1",
  "description": "Kable system internals",
  "main": "./dist/main.js",
  "author": {
    "name": "Horacio Rivero",
    "email": "horacioriverods@gmail.com"
  },
  "repository": "11ume/kable-internals",
  "license": "MIT",
  "scripts": {
    "test": "node node_modules/ava/cli.js -s",
    "test:watch": "node node_modules/ava/cli.js -w -vs",
    "test:verbose": "node node_modules/ava/cli.js -vs",
    "build": "tsc -p ./tsconfig.json",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "kable-core": "https://github.com/11ume/kable-core.git"
  },
  "ava": {
    "compileEnhancements": false,
    "concurrency": 6,
    "cache": true,
    "files": [
      "!dist",
      "!dev",
      "!test/utils/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "typescript": "^3.7.5",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "ava": "2.4.0"
  }
}